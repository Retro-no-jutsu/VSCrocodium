{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build Bin/DSK/CPR/SNA",
        "type": "process",
        "command": "VSCrocodium\\data\\apps\\rasm.exe",
        "args": [
          "${file}",
          "-oa"
        ],
        "problemMatcher": "$errmatcher-rasm",
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "(SNA) Build&Test",
        "type": "process",
        "group": "build",
        "command": "VSCrocodium\\data\\apps\\cpcec\\cpcec.exe",
        "args": ["${relativeFileDirname}\\${fileBasenameNoExtension}.sna"],
        "dependsOn": "Build Bin/DSK/CPR/SNA",
      },
      {
        "label": "(CPR) Build&Test",
        "type": "process",
        "group": "build",
        "command": "VSCrocodium\\data\\apps\\cpcec\\cpcec.exe",
        "args": ["${relativeFileDirname}\\${fileBasenameNoExtension}.cpr"],
        "dependsOn": "Build Bin/DSK/CPR/SNA",
      }
    ]
  }
